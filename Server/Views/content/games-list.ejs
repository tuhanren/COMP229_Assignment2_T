<h1><strong><%= title %></strong></h1>

<div class="row">
      <div class="col">
            <table class="table table-dark table-striped table-hover" id="gametable">
                  <thead class="table-dark">
                        <tr>
                              <th scope="col">#</th>
                              <th scope="col">Name</th>
                              <th scope="col">Genre</th>
                              <th scope="col">Developer</th>
                              <th scope="col">Cost</th>
                        </tr>
                  </thead>
                  <tbody>    
                        <% for (let count = 0; count < games.length; count++) { %>
                              <!-- Repeatable Row Template -->
                              <tr>
                                    <th scope="row"><%= count + 1 %></th>
                                    <td><%= games[count].name %></td>
                                    <td><%= games[count].genre %></td>
                                    <td><%= games[count].developer %></td>
                                    <td><%= games[count].cost %></td>
                              </tr>
                        <% } %>
                  </tbody>
            </table>
      </div>
</div>